    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.holyong.autogenerator.entity.SplitScreen">
        <id column="id" property="id" />
        <result column="smid" property="smid" />
        <result column="split_screen_status" property="splitScreenStatus" />
        <result column="split_screen_type" property="splitScreenType" />
