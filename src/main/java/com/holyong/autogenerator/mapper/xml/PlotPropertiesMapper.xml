<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.holyong.autogenerator.mapper.PlotPropertiesMapper">



    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.holyong.autogenerator.entity.PlotProperties">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="content" property="content" />
        <result column="coordinates" property="coordinates" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, content, coordinates
    </sql>

    <!--自定义关联查询-->
    <!-- 表名 -->
    <sql id="t_name">plot_properties</sql>
    <!-- 别名 -->
    <sql id="t_alias">plot_properties as properties</sql>
    <!--外键表关联定义 -->
    <sql id="join"></sql>
</mapper>
